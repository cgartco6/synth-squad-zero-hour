    <script>
        // AI Team Data
        const aiTeam = [
            {
                name: "Striker",
                role: "Combat Specialist",
                icon: "fa-crosshairs",
                accuracy: 92,
                speed: 87,
                description: "Expert in all weapon types with enhanced targeting systems."
            },
            {
                name: "Guardian",
                role: "Defense Specialist",
                icon: "fa-shield-alt",
                defense: 95,
                health: 78,
                description: "Provides team protection and can deploy defensive structures."
            },
            {
                name: "Medic",
                role: "Support Specialist",
                icon: "fa-first-aid",
                healing: 90,
                speed: 85,
                description: "Heals and revives team members with advanced medical AI."
            },
            {
                name: "Cipher",
                role: "Hacking Specialist",
                icon: "fa-code",
                hacking: 88,
                stealth: 82,
                description: "Can disable enemy systems and unlock strategic advantages."
            }
        ];

        // Initialize the page
        document.addEventListener('DOMContentLoaded', function() {
            renderAITeam();
            updateStats();
            
            // Simulate live data updates
            setInterval(updateStats, 5000);
        });

        // Render AI Team Members
        function renderAITeam() {
            const teamGrid = document.querySelector('.team-grid');
            teamGrid.innerHTML = '';
            
            aiTeam.forEach(member => {
                const card = document.createElement('div');
                card.className = 'ai-card';
                
                // Determine stats based on role
                let statsHTML = '';
                if (member.role === 'Combat Specialist') {
                    statsHTML = `
                        <div class="stat">
                            <div class="stat-value">${member.accuracy}%</div>
                            <div class="stat-name">Accuracy</div>
                        </div>
                        <div class="stat">
                            <div class="stat-value">${member.speed}%</div>
                            <div class="stat-name">Speed</div>
                        </div>
                    `;
                } else if (member.role === 'Defense Specialist') {
                    statsHTML = `
                        <div class="stat">
                            <div class="stat-value">${member.defense}%</div>
                            <div class="stat-name">Defense</div>
                        </div>
                        <div class="stat">
                            <div class="stat-value">${member.health}%</div>
                            <div class="stat-name">Health</div>
                        </div>
                    `;
                } else if (member.role === 'Support Specialist') {
                    statsHTML = `
                        <div class="stat">
                            <div class="stat-value">${member.healing}%</div>
                            <div class="stat-name">Healing</div>
                        </div>
                        <div class="stat">
                            <div class="stat-value">${member.speed}%</div>
                            <div class="stat-name">Speed</div>
                        </div>
                    `;
                } else if (member.role === 'Hacking Specialist') {
                    statsHTML = `
                        <div class="stat">
                            <div class="stat-value">${member.hacking}%</div>
                            <div class="stat-name">Hacking</div>
                        </div>
                        <div class="stat">
                            <div class="stat-value">${member.stealth}%</div>
                            <div class="stat-name">Stealth</div>
                        </div>
                    `;
                }
                
                card.innerHTML = `
                    <div class="ai-icon">
                        <i class="fas ${member.icon}"></i>
                    </div>
                    <h3>${member.name}</h3>
                    <span class="ai-role">${member.role}</span>
                    <p>${member.description}</p>
                    <div class="ai-stats">
                        ${statsHTML}
                    </div>
                    <button class="cta-button" style="padding: 8px 20px; font-size: 0.9rem;">Upgrade</button>
                `;
                
                teamGrid.appendChild(card);
            });
        }

        // Update stats with random fluctuations to simulate live data
        function updateStats() {
            // Update AI team stats with small random fluctuations
            aiTeam.forEach(member => {
                if (member.accuracy) member.accuracy = Math.min(100, Math.max(80, member.accuracy + Math.floor(Math.random() * 3 - 1)));
                if (member.speed) member.speed = Math.min(100, Math.max(80, member.speed + Math.floor(Math.random() * 3 - 1)));
                if (member.defense) member.defense = Math.min(100, Math.max(80, member.defense + Math.floor(Math.random() * 3 - 1)));
                if (member.health) member.health = Math.min(100, Math.max(80, member.health + Math.floor(Math.random() * 3 - 1)));
                if (member.healing) member.healing = Math.min(100, Math.max(80, member.healing + Math.floor(Math.random() * 3 - 1)));
                if (member.hacking) member.hacking = Math.min(100, Math.max(80, member.hacking + Math.floor(Math.random() * 3 - 1)));
                if (member.stealth) member.stealth = Math.min(100, Math.max(80, member.stealth + Math.floor(Math.random() * 3 - 1)));
            });
            
            renderAITeam();
        }

        // Switch between tabs
        function switchTab(tabName, element) {
            // Deactivate all tabs
            const tabs = document.querySelectorAll('.tab');
            tabs.forEach(tab => tab.classList.remove('active'));
            
            // Activate clicked tab
            element.classList.add('active');
            
            // Hide all tab content
            const tabContents = document.querySelectorAll('.tab-content');
            tabContents.forEach(content => content.classList.remove('active'));
            
            // Show selected tab content
            document.getElementById(`${tabName}-tab`).classList.add('active');
        }

