# Server Configuration
NODE_ENV=production
PORT=5000
FRONTEND_URL=https://yourdomain.com

# Database Configuration
DB_HOST=localhost
DB_USER=synth_user
DB_PASSWORD=your_secure_password
DB_NAME=synth_squad
DB_PORT=3306

# JWT Configuration
JWT_SECRET=your_super_secret_jwt_key
JWT_EXPIRES_IN=7d

# Payment Gateway Configuration
PAYPAL_MODE=sandbox
PAYPAL_CLIENT_ID=your_paypal_client_id
PAYPAL_CLIENT_SECRET=your_paypal_client_secret

# South African Banks API Keys
FNB_CLIENT_ID=your_fnb_client_id
FNB_CLIENT_SECRET=your_fnb_client_secret
ABSA_API_KEY=your_absa_api_key
STANDARD_BANK_API_KEY=your_standard_bank_api_key
NEDBANK_API_KEY=your_nedbank_api_key
CAPITEC_API_KEY=your_capitec_api_key

# Crypto Configuration
ETHEREUM_RPC_URL=https://mainnet.infura.io/v3/your_infura_project_id
ETHEREUM_PRIVATE_KEY=your_ethereum_private_key

# VALR Configuration
VALR_API_KEY=your_valr_api_key
VALR_API_SECRET=your_valr_api_secret

# Social Media API Keys
TIKTOK_ACCESS_TOKEN=your_tiktok_access_token
INSTAGRAM_ACCESS_TOKEN=your_instagram_access_token
FACEBOOK_ACCESS_TOKEN=your_facebook_access_token
TWITTER_API_KEY=your_twitter_api_key
TWITTER_API_SECRET=your_twitter_api_secret
YOUTUBE_API_KEY=your_youtube_api_key

# Email Configuration (for notifications)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_email_password

# CDN Configuration (for asset storage)
CDN_URL=https://your-cdn-url.com
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
S3_BUCKET=your_s3_bucket_name
